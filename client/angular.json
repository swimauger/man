
{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "analytics": false
  },
  "projects": {
    "client": {
      "projectType": "application",
      "root": "src",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "browser": "src/main.ts",
            "index": "src/index.html",
            "outputPath": "out",
            "polyfills": [ "zone.js" ],
            "tsConfig": "tsconfig.json"
          }
        }
      }
    }
  }
}
